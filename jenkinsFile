pipline{
    
    agent any
    steps{
        
        stage ('Compile Stage'){
                  
                  steps{
                      
                      withMaven(maven:'MAVEN_HOME'){
                          sh 'mvn clean compile'
                          
                      }

                  }

              }
               stage ('Testing Stage'){
                  
                  steps{
                      
                      withMaven(maven:'MAVEN_HOME'){
                          sh 'mvn test'
                          
                      }

                  }

              }
   				stage ('Deploye Stage'){
                  
                  steps{
                      
                      withMaven(maven:'MAVEN_HOME'){
                          sh 'mvn deploy'
                          
                      }

                  }

              }

    }

}
